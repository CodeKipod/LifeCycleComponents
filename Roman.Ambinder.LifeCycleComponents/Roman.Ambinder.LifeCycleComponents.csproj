<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Roman Ambinder</Authors>
    <Copyright>Roman Ambinder</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/CodeKipod/LifeCycleComponents</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CodeKipod/LifeCycleComponents</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <Description>Objects' life cycle mangement facilities</Description>
    <PackageTags>LifeCycle</PackageTags>
    <Version>1.0.2</Version>
    <PackageReleaseNotes>Nuggets update</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Roman.Ambinder.DataTypes.OperationResults" Version="1.1.2" />
  </ItemGroup>

</Project>
